{
  "$schema": "https://opencode.ai/config.json",

  /* **************************************************************************
   * PLUGINS
   * ************************************************************************ */
  "plugin": ["opencode-antigravity-auth@beta", "oh-my-opencode"],

  /* **************************************************************************
   * PROVIDERS
   * ************************************************************************ */
  "provider": {
    "anthropic": {
      "models": {
        "claude-opus-4-5": {
          "name": "*Claude Opus 4.5",
          "variants": {
            "off": { "thinking": { "type": "disabled" } },
            "1k": { "thinking": { "type": "enabled", "budgetTokens": 1024 } },
            // Anthropic suggests 10k is good
            "10k": { "thinking": { "type": "enabled", "budgetTokens": 10000 } },
          },
        },
      },
    },

    // GLM Coding Plan (Z.ai)
    "zai-coding-plan": {
      "models": {
        "glm-4.7": {
          "name": "*GLM 4.7",
        },
      },
    },

    // OpenAI
    "openai": {
      "models": {
        "gpt-5.2": {
          "name": "*GPT 5.2",
          "variants": {
            "none": { "disabled": true },
            "minimal": { "disabled": true },
            "low": { "disabled": true },
            "medium": {
              "reasoningEffort": "medium",
              "reasoningSummary": "auto",
              "textVerbosity": "medium",
            },
            "high": {
              "reasoningEffort": "high",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium",
            },
            "xhigh": { "disabled": true },
            "max": { "disabled": true },
          },
        },
        "gpt-5.2-codex": {
          "name": "*GPT 5.2 Codex",
          "variants": {
            "none": { "disabled": true },
            "minimal": { "disabled": true },
            "low": { "disabled": true },
            "medium": {
              "reasoningEffort": "medium",
              "reasoningSummary": "auto",
              "textVerbosity": "medium",
            },
            "high": {
              "reasoningEffort": "high",
              "reasoningSummary": "detailed",
              "textVerbosity": "medium",
            },
            "xhigh": { "disabled": true },
            "max": { "disabled": true },
          },
        },
      },
    },

    // MiniMax
    "minimax": {
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "https://api.minimax.io/anthropic/v1",
      },
      "models": {
        "MiniMax-M2.1": {
          "name": "*MiniMax-M2.1",
        },
      },
    },

    // Chutes - models have reasoning enabled by default
    "chutes": {
      "npm": "@ai-sdk/openai-compatible",
      "api": "https://llm.chutes.ai/v1",
      "models": {
        "zai-org/GLM-4.7-TEE": {
          "name": "*GLM 4.7",
          "reasoning": true,
          "variants": { "reasoning": {} },
        },
        "MiniMaxAI/MiniMax-M2.1-TEE": {
          "name": "*MiniMax M2.1",
          "reasoning": true,
          "variants": { "reasoning": {} },
        },
      },
    },

    // Google Antigravity
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingLevel": "low" },
            "high": { "thinkingLevel": "high" },
          },
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "minimal": { "thinkingLevel": "minimal" },
            "low": { "thinkingLevel": "low" },
            "medium": { "thinkingLevel": "medium" },
            "high": { "thinkingLevel": "high" },
          },
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          },
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          },
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
      },
    },
  },

  /* **************************************************************************
   * BINDS
   * ************************************************************************ */
  "keybinds": {
    "leader": "ctrl+x",
    "session_compact": "none",
    "session_share": "none",
  },

  /* **************************************************************************
   * PERMISSIONS
   * ************************************************************************ */
  "permission": {
    "edit": "allow",
    "webfetch": "allow",
    "bash": {
      // python
      "python *": "deny",
      "python3 *": "deny",
      "uv *": "allow",

      // git
      "git merge *": "ask",
      "git pull *": "ask",
      "git push *": "ask",

      // `gh`
      "gh issue close *": "ask",
      "gh issue comment *": "ask",
      "gh issue create *": "ask",
      "gh issue delete *": "ask",
      "gh issue edit *": "ask",
      "gh pr close *": "ask",
      "gh pr comment *": "ask",
      "gh pr create *": "ask",
      "gh pr edit *": "ask",
      "gh pr merge *": "ask",
      "gh pr review *": "ask",
      "gh repo delete *": "ask",

      // allow all commands in ~/.config/opencode
      "~/.config/opencode/*": "allow",
    },
    "external_directory": {
      "*": "ask",
      "~/.config/opencode": "allow",
      "~/.config/opencode/*": "allow",
    },
  },

  /* **************************************************************************
   * MCPs
   * ************************************************************************ */
  "mcp": {
    "exa": {
      "enabled": true,
      "type": "remote",
      "url": "https://mcp.exa.ai/mcp?tools=web_search_exa,get_code_context_exa,crawling_exa,deep_researcher_start,deep_researcher_check",
    },
    "sequentialthinking": {
      "enabled": false,
      "type": "remote",
      "url": "https://remote.mcpservers.org/sequentialthinking/mcp",
    },
    "deepwiki": {
      "enabled": true,
      "type": "remote",
      "url": "https://mcp.deepwiki.com/mcp",
    },
    "chrome-devtools-mcp": {
      "enabled": false,
      "type": "local",
      "command": ["bun", "x", "chrome-devtools-mcp@latest"],
    },
    "reddit-mcp-buddy": {
      "enabled": false,
      "type": "local",
      "command": ["bun", "x", "reddit-mcp-buddy"],
    },
  },

  /* **************************************************************************
   * MISC
   * ************************************************************************ */
  "default_agent": "plan",
  "autoupdate": false,
  "share": "manual",
  "experimental": {
    "disable_paste_summary": true,
    "continue_loop_on_deny": true,
  },
}
